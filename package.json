{
  "name": "software-contable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:db": "docker compose --env-file=.env.local up --force-recreate -d",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "migrate:prod": "npx dotenv -e .env.local -- prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "prisma": "^5.6.0",
    "typescript": "^5"
  }
}
